app.get('/secure/customer-platform/logout', async (c) => {
    return c.html(`
        <!DOCTYPE html>
        <html lang="en">
        <head>
            <meta charset="UTF-8">
            <title>Logout</title>
        </head>
        <body>
            <h2>Logout Successful</h2>
        </body>
        </html>
    `);
});